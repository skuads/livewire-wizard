{
  "name": "skuads/livewire-wizard-beta",
  "description": "Laravel Livewire Wizard (Multi-Step Form) component",
  "keywords": [
    "laravel",
    "livewire",
    "multi-steps",
    "wizard",
    "form"
  ],
  "homepage": "https://github.com/vildanbina/livewire-wizard",
  "license": "MIT",
  "authors": [
    {
      "name": "Vildan Bina",
      "email": "vildanbina@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "livewire/livewire": "^3.0",
    "spatie/laravel-package-tools": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Skuads\\LivewireWizard\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Skuads\\LivewireWizard\\LivewireWizardServiceProvider"
      ],
      "aliases": {
        "LivewireWizard": "Skuads\\LivewireWizard\\Facades\\LivewireWizard"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
